# Rerank 重排查询相关
RerankMultiplier=2
RerankUrl=your url
RerankApi=your api
RerankModel=Qwen/Qwen3-Reranker-8B
RerankMaxTokensPerBatch=30000

# AI级回忆相关
AIMemoModel=
AIMemoBatch=5
AIMemoUrl=your url
AIMemoApi=your api
AIMemoMaxTokensPerBatch=60000
AIMemoPrompt=AIMemoPrompt.txt

# 缓存配置清单
RAG_CACHE_MAX_SIZE=100           
RAG_CACHE_TTL_MS=3600000 

# 向量缓存配置
EMBEDDING_CACHE_MAX_SIZE=500       
EMBEDDING_CACHE_TTL_MS=7200000      

# 开启结果缓存
RAG_QUERY_CACHE_ENABLED=false

# RAG_PARSE_DEPTH: 解析深度上限
# 当使用本插件的语法将内容解析出来后，再继续对解析出来后的内容继续解析，直到到达深度上限。
# 默认为 1 (不递归)。设为 2 则解析一次后再解析一次。
RAG_PARSE_DEPTH=1